			<tuv lang="en">
				<seg>Login</seg>
			</tuv>
			<tuv lang="fi">
				<seg>Kirjaudu</seg>
			</tuv>
			<tuv lang="se">
				<seg>Logga in</seg>
			</tuv>
		</tu>
